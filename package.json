{
    "name": "pluggable-autocomplete",
    "displayName": "pluggable-autocomplete",
    "description": "Suggestions using data from custom plugins",
    "version": "0.0.2",
    "publisher": "Sammons",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Sammons/pluggable-autocomplete.git"
    },
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*",
        "onCommand:pluggable-autocomplete.initialize"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "pluggable-autocomplete.initialize",
                "title": "Create Sample Pluggable Autocomplete"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "@types/lodash": "^4.14.71",
        "lodash": "^4.17.4",
        "source-map-support": "^0.4.15"
    }
}
